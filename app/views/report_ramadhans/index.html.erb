<p id="notice"><%= notice %></p>

<h1>Report Ramadhans</h1>

<table>
  <thead>
    <tr>
      <th>Puasa</th>
      <th>Tadarus</th>
      <th>Tarawih</th>
      <th>Subuh</th>
      <th>Dzuhur</th>
      <th>Ashar</th>
      <th>Magrib</th>
      <th>Isya</th>
      <th>Duha</th>
      <th>Tahajud</th>
      <th>Taubat</th>
      <th>Hajat</th>
      <th>Alasan</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @report_ramadhans.each do |report_ramadhan| %>
      <tr>
        <td><%= report_ramadhan.puasa %></td>
        <td><%= report_ramadhan.tadarus %></td>
        <td><%= report_ramadhan.tarawih %></td>
        <td><%= report_ramadhan.subuh %></td>
        <td><%= report_ramadhan.dzuhur %></td>
        <td><%= report_ramadhan.ashar %></td>
        <td><%= report_ramadhan.magrib %></td>
        <td><%= report_ramadhan.isya %></td>
        <td><%= report_ramadhan.duha %></td>
        <td><%= report_ramadhan.tahajud %></td>
        <td><%= report_ramadhan.taubat %></td>
        <td><%= report_ramadhan.hajat %></td>
        <td><%= report_ramadhan.alasan %></td>
        <td><%= report_ramadhan.student_id %></td>
        <td><%= link_to 'Show', report_ramadhan %></td>
        <td><%= link_to 'Edit', edit_report_ramadhan_path(report_ramadhan) %></td>
        <td><%= link_to 'Destroy', report_ramadhan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report Ramadhan', new_report_ramadhan_path %>
